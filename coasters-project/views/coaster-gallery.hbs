<div class="container">
    <div class="row">
        {{#each allCoasters}}
        <div class="col-md-4">
            <div class="card mb-4 player-card">
                <img src="{{imageUrl}}" class="card-img-top object-fit-scale" alt="{{name}}">
                <div class="card-body">
                    <h5 class="card-title">Name:{{title}}</h5>
                    <p class="card-text">Length:{{length}}</p>
                    <p class="card-text "> Inversions: {{inversions}}</p>
                    <p class="card-text">Description:{{description}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>