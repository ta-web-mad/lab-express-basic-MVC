<div class="container">

    <div class="row">

        {{#each coasters}}

        <div class="col-md-4 coaster-info">

            <img src="{{imageUrl}}" alt="image">

            <h4>{{title}}</h4>

            <p>{{description}}</p>

            <p>Length: {{length}}</p>

            <p>Inversions: {{inversions}}</p>

        </div>

        {{/each}}

    </div>

</div>