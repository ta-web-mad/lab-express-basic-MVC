<h1>The craziest top 6!</h1>

<div class="row cards">
    {{#each coasters}}
    <div class="col-md-4">

        <div class="card" style="width: 18rem;">
            <img src="{{imageUrl}}" class="card-img-top" alt="coasterImage">
            {{!-- <img class="card-img-top" src="" alt="Card image cap"> --}}
            <div class="card-body">
                <h5 class="card-title">{{title}}</h5>
                <p class="card-text">{{description }}</p>
                <p class="card-text"><strong> Length: </strong>{{length}}</p>
                <p class="card-text"><strong> Inversions: </strong>{{inversions}}</p>

            </div>
        </div>
    </div>

    {{/each}}

</div>